{
  "name": "syntax",
  "version": "0.0.1",
  "description": "Syntactic analysis toolkit for education, tracing, and parsers generation",
  "repository": {
    "type": "git",
    "url": "https://github.com/DmitrySoshnikov/syntax.git"
  },
  "bugs": "https://github.com/DmitrySoshnikov/syntax/issues",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist; babel src/ --out-dir dist/ --presets es2015",
    "watch": "babel src/ --out-dir dist/ --watch --presets es2015",
    "test": "jest",
    "prepublish": "jest && npm run build"
  },
  "bin": {
    "syntax": "./bin/syntax"
  },
  "keywords": [
    "parser",
    "LL(1)",
    "LR(1)",
    "LALR(1)",
    "SLR(1)",
    "generator"
  ],
  "author": "Dmitry Soshnikov",
  "license": "MIT Style License",
  "dependencies": {
    "babel-cli": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "nomnom": "^1.8.1",
    "cli-table2": "^0.1.9"
  },
  "devDependencies": {
    "babel": "^6.2.0",
    "babel-jest": "^6.0.1",
    "jest-cli": "^0.8.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "preprocessCachingDisabled": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathDirs": [
      "src",
      "bin"
    ],
    "unmockedModulePathPatterns": [
      "babel"
    ]
  }
}